package code1;

public class Variables {

	public static void main(String[] args) {
		int a = 20;
		float b = 20; //implicit
		float d = 33.9f;
		int c = (int)d; //explicit
		char e = 'A';
		char f= 68; // ASCII
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
		System.out.println(e);
		System.out.println(f);
	}

}
